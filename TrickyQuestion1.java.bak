class TrickyQuestion1{
	public static class Person {
    private int age;

    // Constructor
    public Person() {
    }

    // Getter method for age
    public int getAge() {
        return age;
    }

    // Setter method for age
    public void setAge(int age) {
        this.age = age;
    }
}
public static void main(String[] args) {
  TrickyQuestion1 p = new TrickyQuestion1();
  p.setAge(25);
  change(p);
  System.out.println(p.getAge());
}

static void change(TrickyQuestion1 p) {
  p.setAge(10);
}
}

